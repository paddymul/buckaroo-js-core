    "markdown-loader": "^7.0.0",
    "null-loader": "^4.0.1",
    "prettier": "2.6.2",
    "prism-themes": "^1.6.0",
    "prismjs": "^1.28.0",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.27.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^6.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
